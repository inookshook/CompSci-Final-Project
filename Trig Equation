#include <iostream>
#include <cmath>
#include <iomanip>
#include <string>
#define PI 3.14159265
using namespace std;

double hypo(double h, double a, double b);
double angleA(double A, double a, double b, double h);



double hypo(double h, double a, double b)
{
	
	h = sqrt(pow(a, 2) + pow(b, 2));
	return h;
}
double angleA(double A,double a,double b, double h)
{
	
	A = asin(a / double(h)) || acos(b / double (h));
	 
	
	
	return A*180/PI;
}
double B(double B, double a, double b, double h)
{
	B = asin(b / h);
	B = acos(a / h);
	
	return B;
}


int main()
{												
	double a = {}, b = {}, A = {}, h = {};
	h = hypo(h, a, b);
	 
	
	cout << "Please input a value for a and b. \n";
	cout << "a = ";
	cin >> a;
	cout << endl;
	cout << "b =";
	cin >> b;
	cout << endl;
	cout << "h=" << h << endl;
	
	A = angleA(A, a, b, h);
	
	cout << "Angle A:" << A << endl;
	

	return 0;
}

