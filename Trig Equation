#include <iostream>
#include <cmath>
#include <iomanip>
#define PI 3.14159265
#include <limits>
using namespace std;
int main()
{
	const int sine = 5;
	const int cosine = 5;
	int i, j,e;
	double tangent[sine][cosine] = { {0, 0.5, 0.707106, 0.866025, 1},
									{1,0.866025, 0.707106, 0.5, 0} };
	
	float result = 0;

	
	for (i = 0; i < sine; i++)
	{
		for (e = 0; e < cosine; e++)
		{
			
		}
		
	}
	result /= tangent[i][e]; //Results is always 0 instead of an actually decimal number.

	
	const int angle = 5;
	double degrees[angle] = { 0 ,30 ,45 ,60,90 };


	for (j = 0; j < angle; j++)
	{
		cout << "Angle " << degrees[j] << " degrees has a tangent value of = " << result << "." << endl;
	}
	return 0;
	
}
